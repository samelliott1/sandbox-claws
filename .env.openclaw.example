# OpenClaw-specific environment variables
# This file is loaded by the openclaw container

# OpenClaw Logging
OPENCLAW_LOG_LEVEL=INFO
OPENCLAW_LOG_FILE=/app/logs/openclaw.log

# OpenClaw Data Directory
OPENCLAW_DATA_DIR=/app/data

# API Credentials (fill these in with your test account credentials)
# Gmail API
GMAIL_CLIENT_ID=
GMAIL_CLIENT_SECRET=
GMAIL_REFRESH_TOKEN=

# Google Calendar API
GOOGLE_CALENDAR_CLIENT_ID=
GOOGLE_CALENDAR_CLIENT_SECRET=
GOOGLE_CALENDAR_REFRESH_TOKEN=

# Security Settings
OPENCLAW_SANDBOX_MODE=true
OPENCLAW_RATE_LIMIT=10
OPENCLAW_MAX_FILE_SIZE=10485760

# Feature Flags
OPENCLAW_ENABLE_EMAIL=true
OPENCLAW_ENABLE_CALENDAR=true
OPENCLAW_ENABLE_FILE_ACCESS=false
